generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/mongo/client"
}

datasource mongodb {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model post {
    id            String   @id @default(auto()) @map("_id") @mongodb.ObjectId
    createdAt     DateTime @default(now())
    text          String
    @@map("images")
}