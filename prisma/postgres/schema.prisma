//: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/postgres/client"
}

datasource postgresql {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model User {
  id             String  @unique @default(uuid()) @postgresql.Uuid
  email          String  @unique
  first_name     String?
  last_name      String?
  pinned_post_id String  @unique

  @@map("users")
}
